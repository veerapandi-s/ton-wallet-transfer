{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "noImplicitReturns": true,
      "noUnusedLocals": false,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "rootDir": "./src",
      "outDir": "./build",
      "types": [""]
    }
  }